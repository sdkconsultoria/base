{
    "name": "sdkconsultoria/base",
    "type": "library",
    "description": "Funciones basicas y auxiliares, para generar una página web rápidamente",
    "keywords": [
        "laravel",
        "package",
        "base"
    ],
    "license": "MIT",
    "authors": [{
        "name": "Camilo Antonio Rodríguez Cruz",
        "email": "camilo@sdkconsultoria.com",
        "homepage": "https://sdkconsultoria.com",
        "role": "Developer"
    }],
    "require": {
        "php": "^8.0",
        "intervention/image": "^2.6",
        "laravel/fortify": "^1.8",
        "openpay/sdk": "^2.2",
        "hybridauth/hybridauth": "^3.7",
        "sdkconsultoria/api-base": "*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.8"
    },
    "autoload": {
        "files": [],
        "psr-4": {
            "Sdkconsultoria\\Base\\": "src/",
            "Sdkconsultoria\\Base\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": ["Sdkconsultoria\\Base\\InstallProvider"],
            "aliases": {
                "Base": "Sdkconsultoria\\Base\\Facade"
            }
        }
    }
}
